{
  "name": "profile",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://caijin1002.github.io/",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/**/stories/*.stories.js",
      "!<rootDir>/lib/**",
      "!<rootDir>/*.js",
      "!<rootDir>/static/**"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose --runInBand --coverage --watchAll",
    "eject": "react-scripts eject",
    "lint:css": "stylelint **/styled/*.js",
    "lint:js": "eslint --fix \"**/*.{jsx,js}\"",
    "lint:prettier": "prettier \"**/*.{jsx,js}\"",
    "lint": "concurrently \"npm run lint:prettier\" \"npm run lint:css\" \"npm run lint:js\"",
    "storybook": "start-storybook",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  },
